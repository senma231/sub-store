# Cloudflare 配置
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token
CLOUDFLARE_ACCOUNT_ID=your_cloudflare_account_id
CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id

# KV 存储配置
KV_NAMESPACE_ID=your_kv_namespace_id
KV_NAMESPACE_NAME=sub_store_data

# 管理员配置
ADMIN_TOKEN=your_admin_token_here
ADMIN_PASSWORD=your_admin_password

# 应用配置
APP_NAME=Sub-Store
APP_URL=https://your-domain.workers.dev
FRONTEND_URL=https://your-username.github.io/sub-store

# 订阅配置
DEFAULT_SUBSCRIPTION_INTERVAL=86400
MAX_NODES_PER_SUBSCRIPTION=100
ENABLE_NODE_HEALTH_CHECK=true
HEALTH_CHECK_INTERVAL=3600

# 安全配置
JWT_SECRET=your_jwt_secret_key
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=3600

# 统计配置
ENABLE_ANALYTICS=true
ANALYTICS_RETENTION_DAYS=30

# 开发配置
NODE_ENV=production
DEBUG=false
LOG_LEVEL=info
