{
  "name": "sub-store-workers",
  "version": "2.0.0",
  "description": "Sub-Store Cloudflare Workers API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "db:create": "wrangler d1 create sub-store-db",
    "db:migrate": "wrangler d1 execute sub-store-db --file=./schema.sql",
    "db:migrate:dev": "wrangler d1 execute sub-store-db --local --file=./schema.sql"
  },
  "dependencies": {
    "hono": "^3.12.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.19.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
